{
  "name": "cardboardvr_experiment",
  "version": "1.0.0",
  "scripts": {
    "start": "echo \"https://localhost:1235\" && parcel src/index.html --https --cert localhost.pem --key localhost-key.pem --port 1235 --host 0.0.0.0",
    "build": "parcel build src/index.html --public-url ./",
    "deploy": "npm run build && gh-pages -d dist && echo \"deployed to: https://radogit.github.io/cocoarui-vr/\" && echo \"check status on: https://github.com/radogit/cocoarui-vr/deployments\"",
    "clean": "rm -rf dist .parcel-cache",
    "generate-cert": "./scripts/generate-cert.sh"
  },
  "dependencies": {
    "jsqr": "^1.4.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.16.0",
    "@parcel/transformer-raw": "^2.16.0",
    "@parcel/transformer-webmanifest": "^2.16.0",
    "gh-pages": "^6.3.0",
    "parcel": "^2.16.0"
  },
  "targets": {
    "default": {
      "publicUrl": "./",
      "distDir": "dist"
    }
  }
}
